{
  "family": "ardalo-platform",
  "executionRoleArn": "arn:aws:iam::711138541205:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "footer-service",
      "image": "711138541205.dkr.ecr.eu-central-1.amazonaws.com/footer-service:latest",
      "cpu": 256,
      "memoryReservation": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 8082
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ardalo-platform",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512"
}
